{
  "name": "menumingle",
  "version": "1.0.0",
  "description": "MenuMingle is the graduation project of team 6 from the GazaSkyGeeks Code Academy bootcamp.",
  "main": "index.js",
  "scripts": {
    "init": "npm i && npm run prepare && cd client && npm i",
    "test": "cross-env NODE_ENV=test jest --forceExit --testTimeout=30000 --detectOpenHandles --runInBand",
    "build": "npx tsc",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "lint": "cross-env eslint . --ext .ts",
    "lint-fix": "cross-env eslint . --ext .ts --fix",
    "dev": "cross-env NODE_ENV=development npx nodemon server/index.ts",
    "prepare": "husky install",
    "db-build": "cross-env NODE_ENV=development ts-node server/db/build.ts",
    "seed-build": "cross-env NODE_ENV=development ts-node server/db/seed.ts"
  },
  "keywords": [],
