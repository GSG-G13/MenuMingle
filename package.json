{
  "name": "MenuMingle",
  "version": "1.0.0",
  "description": "MenuMingle is the graduation project of team 6 from the GazaSkyGeeks Code Academy bootcamp.",
  "main": "server.js",
  "repository": "https://github.com/GSG-G13/MenuMingle.git",
  "author": "Mustafa Salem <mustafaissasalem@gmail.com>",
  "license": "MIT",
  "private": false,
  "script": {
    "init": "npm i && npm run prepare && cd client && npm i",
    "build": "build:types && build:js",
    "build:js": "esbuild server/index.ts --bundle --platform=node --target=node18-12 --outfile=server.js",
    "build:types": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --runInBand",
    "lint": "cross-env TIMING=1 eslint --ext js,ts server",
    "lint:fix": "cross-env TIMING=1 eslint --ext js,ts --fix server",
    "dev": "cross-env NODE_ENV=deveopment ts-node-dev --transpile-only server",
    "start": "cross-env NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "prettier": "^2.8.8",
    "sequelize": "^6.32.0",
    "sequelize-i18n": "^1.1.0"
  }
}
